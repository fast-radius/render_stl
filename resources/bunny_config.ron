(
    output_path: "bunny.png",
    width: 250,
    height: 250,
    crop: true,
    sampler: StratifiedSampler(
        x_strata_count: 2,
        y_strata_count: 2,
        jitter: true,
    ),
    part: Part(
        stl_path: "../resources/bunny.stl",
        material: MatteMaterial(
            kd: Rgb(r: 0.8, g: 0.8, b: 0.8),
            sigma: 0.3,
        ),
        handedness: RightHanded,
