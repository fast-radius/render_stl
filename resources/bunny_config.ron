    lights: [
        PointLight(
            position: Spherical(radius: 5.0, theta: 50, phi: -130),
            intensity: Rgb(r: 85.0, g: 85.0, b: 85.0),
        ),
        PointLight(
            position: Spherical(radius: 10.0, theta: 70, phi: -45),
            intensity: Rgb(r: 60.0, g: 60.0, b: 60.0),
        ),
    ],
    camera: OrthographicCamera(
            position: Spherical(radius: 1.0, theta: 60.0, phi: -100),
            z_near: 0.0,
            z_far: 10.0,
    )
